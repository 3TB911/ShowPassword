{
    "name": "ShowPassword",
    "version": "1.1.4",
    "manifest_version": 2,
    "default_locale": "en",
    "description": "__MSG_extension_description__",
    "content_scripts": [{
            "all_frames": true,
            "js": ["showPassword.js"],
            "matches": ["http://*/*", "https://*/*"]
        }],
    "icons": {
        "128": "icon.png",
        "64": "icon.png",
        "32": "icon32.png",
        "16": "icon32.png"
    },
    "options_page": "options.html",
    "permissions": ["storage"]
}
